//NAME: Simple generics
//DESC:
//BEGIN CODE

extern fn printf(fmt: *u8, ...) -> i32;

fn add<A, B>(a: A, b: B) -> B {
    return b + a;
}


fn main() -> i32 {
    let a: f32 = add::<i32, f32>(7, 7.5);
    let b: i32 = add::<i32, i32>(3, 4);
    printf("%f\n", a);
    printf("%d %d\n", b, add::<i32, i32>(10, 20));
    return 0;
}


