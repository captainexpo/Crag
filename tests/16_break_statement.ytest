NAME: Break Statement 
DESC: Test whether the "break" statement works in loops 
BEGIN CODE

extern fn printf(str: *u8, ...) -> i32; 

fn main(argc: i32, argv: **u8) -> i32 {
  let i: i32 = 0;
  while (true){
    while (true) break;
    if (i == 3) break; 
    printf("%d ", i);
    i = i + 1;
  }
  printf("\nDone\n");
  return 0;
}

END CODE
BEGIN EXPECT
0 1 2 \nDone\n
END EXPECT
