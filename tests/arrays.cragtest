extern fn printf(str: *u8, ...) -> i32;

fn main(argc: i32, argv: **u8) -> i32 {
  let arr: [5]u8 = {0};
  for (let i: i32 = 0; i < 5; i+=1) {
    arr[i] = i;
  }
  for (let j: i32 = 0; j < 5; j+=1) {
    printf("%d",arr[j]);
  }
  return 0;
}

