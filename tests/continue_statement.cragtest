//NAME: Continue Statement
//DESC: Test whether the "continue" statement works in loops
//BEGIN CODE

extern fn printf(str: *u8, ...) -> i32;

fn main(argc: i32, argv: **u8) -> i32 {
  let i: i32 = 0;
  while (i < 5){
    i = i + 1;
    if (i == 2) continue;
    printf("%d ", i);
  }
  printf("\nDone\n");
  return 0;
}

//END CODE
//BEGIN EXPECT
//1 3 4 5 \nDone\n
//END EXPECT
