//NAME: Arrays
//DESC: Test the array type
//BEGIN CODE

extern fn printf(str: *u8, ...) -> i32;

fn main(argc: i32, argv: **u8) -> i32 {
  let a: i32 = 1;
  a <<= 5;
  a ^= 0xFAFAFAFA;
  a >>= 2;
  a = ((a | 0xBAFAFAFA) & (a * 7));
  a ^= a - 5;
  printf("%d\n", a);
  a = ~a + 1;
  printf("%d\n", a);

  // Logical operations b/c why not
  let c: bool = true;
  let d: bool = false;

  printf("%d\n", (c && d) as i32);
  printf("%d\n", (c || d) as i32);
  printf("%d\n", (!(c || d) && true) as i32);
  return 0;
}

