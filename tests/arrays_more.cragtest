//NAME: Arrays
//DESC: Test the array literals and stuff
//BEGIN CODE

extern fn printf(fmt: *u8, ...) -> i32;

fn main(argc: i32) -> i32 {
    let arr: []u8 = {'H','E','L','L','O',0};
    other(arr.ptr);
    printf("%s\n", arr.ptr);
    printf("Len: %d\n", arr.len);
    return 0;
}

fn other(arr: *u8) -> u8 {
    arr[0] = 'B';
    return arr[0];
}


