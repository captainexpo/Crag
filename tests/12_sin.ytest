NAME: Sin/Cos Example (trimmed)
DESC: Deterministic subset of examples/sin.y: verify sine values for common angles.
BEGIN CODE

extern fn printf(format: *const u8, ...) -> i32;
const PI = 3.14159;

fn sin(x: f64) -> f64 {
  let term = x;
  let sum = term;
  let n: f64 = 1;
  while (n as i32 < 10) {
      term = -term * x * x / (2 * n * (2 * n + 1.0));
      sum = sum + term;
      n = n + 1;
  }
  return sum;
}

fn main(argc: i32, argv: *const *const u8) -> i32 {
    printf("sin(PI/2): %f\n", sin(PI / 2.0));
    printf("sin(PI/6): %f\n", sin(PI / 6.0));
    printf("sin(PI/4): %f\n", sin(PI / 4.0));
    return 0;
}

END CODE
BEGIN EXPECT
sin(PI/2): 1.000000
sin(PI/6): 0.500000
sin(PI/4): 0.707107
END EXPECT
